    associatedTextSection: {fileID: 616316804}
    referenceNames: []
    checkpointFlag: 0
  - text: '"I''m not done talking to you!"'
    associatedTextSection: {fileID: 1533042065}
    referenceNames: []
    checkpointFlag: 0
  - text: '"What makes you think you can tell me what to do?"'
    associatedTextSection: {fileID: 1354332058}
